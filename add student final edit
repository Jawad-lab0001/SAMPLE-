student_list = {}
student_age = int(input("enter your age:"))
for x in range,{student_age}:
    student_name =input("enter the students name :")
    student_ID =int(input("Enter the student ID :"))
    Course = input("enter the course :")
    student_list [student_age] = student_name, student_ID,   Course,
    print((type(student_list)))
    if student_list != student_name != student_ID != Course:
        print("student Added", "Student name:", {student_name},"Student ID:",  {student_ID}, "Course:", {Course})
    else:
        print("student not added")
        
    
    
